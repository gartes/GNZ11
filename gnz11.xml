<?xml version="1.0" encoding="utf-8"?>
<extension type="library" method="upgrade" version="3.0">
    <name>GNZ11</name>
    <libraryname>GNZ11</libraryname>
    <author>Gartes</author>
    <creationDate>21.09.2020</creationDate>
    <copyright>2019 (C) Gartes</copyright>
    <license>GNU/GPLv3</license>
    <authorEmail>sad.net79@gmail.com</authorEmail>
    <authorUrl>https://ru.wikipedia.org/wiki/GN-z11</authorUrl>
    <version>0.4.8</version>
    <description>Library GN-z11 by Gartes</description>
    <files>
        <folder>Api</folder>
        <folder>assets</folder>
        <folder>Core</folder>
        <folder>Document</folder>
        <folder>Extensions</folder>
        <folder>layouts</folder>
        <folder>sql</folder>
        <file>gnz11.xml</file>
        <file>GNZ11_update_server.xml</file>
        <file>README.md</file>
    </files>
    <install> <!-- Runs on install -->
        <sql>
            <file driver="mysql" charset="utf8">sql/install.mysql.utf8.sql</file>
        </sql>
    </install>
    <uninstall> <!-- Runs on uninstall -->
        <sql>
            <file driver="mysql" charset="utf8">sql/uninstall.mysql.utf8.sql</file>
        </sql>
    </uninstall>
    <update> <!-- Runs on update; New since J2.5 -->
        <schemas>
            <schemapath type="mysql">sql/updates/mysql</schemapath>
        </schemas>
    </update>
    <updateservers>
        <!-- Note: No spaces or linebreaks allowed between the server tags -->
        <server type="extension" enabled="1" priority="1" name="Lib GNZ11 Update Site">https://raw.githubusercontent.com/gartes/GNZ11/master/GNZ11_update_server.xml</server>
    </updateservers>
</extension>

